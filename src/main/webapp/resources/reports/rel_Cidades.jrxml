	<queryString>
		<![CDATA[SELECT
     cidade.nome,
     estado.nome as estado,
     estado.sigla,
     cidade.codigo
FROM
     fisioterapia.cidade INNER JOIN fisioterapia.estado ON cidade.estado_codigo = estado.codigo
WHERE
     cidade.nome LIKE $P{nomeCidade}
 AND  estado.codigo LIKE $P{nomeEstado}]]>
